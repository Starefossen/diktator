# Frontend Development Configuration

# API URL for backend
NEXT_PUBLIC_API_URL=http://localhost:8080

# Authentication mode
NEXT_PUBLIC_AUTH_MODE=mock

# Next.js configuration
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=development

# Mock user configuration (only used when NEXT_PUBLIC_AUTH_MODE=mock)
NEXT_PUBLIC_MOCK_USER_ID=mock-user-12345
NEXT_PUBLIC_MOCK_USER_EMAIL=dev@localhost
NEXT_PUBLIC_MOCK_USER_NAME=Development User
